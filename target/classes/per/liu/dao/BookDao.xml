<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="per.liu.dao.BookDao">

    <select id="queryAllBookInfo" resultType="Book">

        select
            id,
            bId,
            name,
            ISBN,
            author,
            price,
            publisher,
            pub_time,
            storage_time,
            amount
        from tbl_books;



    </select>

</mapper>
