<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="per.liu.dao.ApplicationDao">

    <insert id="addAppli">

            insert into tbl_application(

                tch_id,
                book_id,
                appli_amount,
                class_id

            )  values (

                #{tch_id},
                #{book_id},
                #{appli_amount},
                #{class_id}

            )



    </insert>


</mapper>
